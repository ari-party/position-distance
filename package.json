    "clone": "^2.1.2",
    "flag-icons": "^7.2.3",
    "immer": "^10.1.1",
    "is-image": "^4.0.0",
    "klaw": "^4.1.0",
    "ky": "^1.3.0",
    "merge-sx": "^2.0.2",
    "next": "14.2.4",
    "next-intl": "^3.15.2",
    "react": "^18.3.1",
