    "@types/clone": "^2.1.4",
    "@types/klaw": "^3.0.6",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
